    <script type="text/javascript">
        $(document).ready(function () {

            //----------------------------------------------- Set Active Branch On Page Load

            var activeTabID = $("input[id$='Content_Hidden_Active_Course_Type']").val();
            if (activeTabID != "") {
                $("#div-Courses-" + activeTabID).show();

                $("#ulCourses a").removeClass("Selected");
                $("a[href='#" + activeTabID + "']").addClass("Selected");
            }

            //----------------------------------------------- Select A Branch

            $("#ulCourses a").click(function (e) {
                e.preventDefault();

                var tabID = $(this).attr("href").substr(1);
                $("input[id$='Content_Hidden_Active_Course_Type']").val(tabID);

                $("div[id^='div-Courses-']").fadeOut(200);
                $("#div-Courses-" + tabID).fadeIn(300);

                $("#ulCourses a").removeClass("Selected");
                $(this).addClass("Selected");
            });


            //----------------------------------------------- Set Active Branch On Page Load

            var activeTabID = $("input[id$='Content_Hidden_Active_SPCourse_Type']").val();
            if (activeTabID != "") {
                $("#div-SPCourses-" + activeTabID).show();

                $("#ulSPCourses a").removeClass("Selected");
                $("a[href='#" + activeTabID + "']").addClass("Selected");
            }

            //----------------------------------------------- Select A Branch

            $("#ulSPCourses a.trains").click(function (e) {
                e.preventDefault();

                var tabID = $(this).attr("href").substr(1);
                $("input[id$='Content_Hidden_Active_SPCourse_Type']").val(tabID);

                $("div[id^='div-SPCourses-']").fadeOut(200);
                $("#div-SPCourses-" + tabID).fadeIn(300);

                $("#ulSPCourses a").removeClass("Selected");
                $(this).addClass("Selected");
            });



            //----------------------------------------------- Set Active Branch On Page Load

            var activeTabID = $("input[id$='Content_Hidden_Active_PCourse_Type']").val();
            if (activeTabID != "") {
                $("#div-PCourses-" + activeTabID).show();

                $("#ulPCourses a").removeClass("Selected");
                $("a[href='#" + activeTabID + "']").addClass("Selected");
            }

            //----------------------------------------------- Select A Branch

            $("#ulPCourses a.tab").click(function (e) {
                e.preventDefault();

                var tabID = $(this).attr("href").substr(1);
                $("input[id$='Content_Hidden_Active_PCourse_Type']").val(tabID);

                $("div[id^='div-PCourses-']").fadeOut(200);
                $("#div-PCourses-" + tabID).fadeIn(300);

                $("#ulPCourses a").removeClass("Selected");
                $(this).addClass("Selected");
            });
        });

        $(document).ready(function () {

            //----------------------------------------------- View PDF

            //انتخاب لینک با نام مودال
            $('a[data-name=pdfModal]').click(function (e) {
                //لغو حالت پیش فرض عملکرد لینک
                e.preventDefault();

                //دریافت آیدی عنصر پاپ آپ
                var id = $(this).attr('href');
                var fileName = id.toString().substr(0, id.lastIndexOf('#'));

                if (fileName != '') {
                    $.ajax({
                        type: "POST",
                        url: "/AjaxPages/PDF.aspx/ShowPDF",
                        data: "{fileName: '" + fileName + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (r) {
                            $("#divPopupViewPDFDialog").html(r.d);
                        }
                    });
                }

                id = id.toString().substr(id.lastIndexOf('#'));
                showModal(id);
            });

            //----------------------------------------------- Set First Schedule Course Active On Page Load

            $("#div-Course-Schedule .Course").first().addClass("Selected");
            $("#div-Course-Schedule .CourseDetails").first().show();

            //----------------------------------------------- Select Special Course Category

            $('body').on('click', '.RunningCourseScroll', (e) => {
                debugger;
                var cat = e.target.name;
                $("input[id$='Content_Hidden_Active_SPCourse_Type']").val(cat);
                if (cat != "") {
                    $("div[id^='div-SPCourses-'").hide();
                    $("#div-SPCourses-" + cat).show();

                    $("#ulSPCourses a").removeClass("Selected");
                    $("a[href='#" + cat + "']").addClass("Selected");
                }
            });

            $("#divSpecialCourses .FlipperColorlyBox a").click(function (e) {
                var categoryID = $(this).attr("name");

                if (categoryID == null)
                    return;

                e.preventDefault();

                var categoryName = $(this).attr("href").substr(1);

                if (categoryName == '')
                    return;

                $("#divSpecialCourseContent").removeAttr("class");
                $("#divSpecialCourseContent").addClass($(this).parent().attr("class").replace("FlipperColorlyBox ", ""));
                $("#divSCContentInside").html('').hide();
                $("#divSpecialCourseContent img[id^='img']").hide();

                //.................... Get Courses

                //.................... Main Description

                if (categoryID == 'SC') {
                    $("#divSCContentInside").html('').hide();
                    $("#divSCMain").fadeIn();
                }

                //.................... Special Courses

                else {

                    $("#divSCMain").hide();
                    $("#divConsultation").hide();
                    $("#divLoader").show();

                    $.ajax({
                        type: "POST",
                        url: "../AjaxPages/Academy.aspx/GetSpecialCourses",
                        data: "{" +
                            "   categoryID : '" + categoryID + "'" +
                            "}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (data) {

                            if (data.d == null) {
                                $("#divSCContentInside").html("عملیات با خطا مواجه شده است. <br />لطفا مجددا تلاش نمایید.").fadeIn();
                            }
                            else {
                                //$("#divSCContentInside").append("<p class='StartDate'>شروع قطعی کلاس ها : اردیبهشت 1398</p>");

                                //.................... Add Course
                                $("#divSCContentInside").attr("style", "");
                                for (var i = 0; i < data.d.length; i++) {
                                    debugger;
                                    //if (categoryID == "7")
                                    //    $("#divSCContentInside").append("<p><a href='../SpecialCourses/ProjectCourses.aspx?SpCourseID=" + data.d[i].ID + "'>" +
                                    //        data.d[i].Title + "</a></p>");
                                    if (categoryID == "5")//Mobile
                                        $("#divSCContentInside").append("<p><a class='RunningCourseScroll' href='#RunningCourseHeader' name='MOB'>" +
                                            data.d[i].Title + "</a></p>");
                                    if (categoryID == "3")//Web
                                        $("#divSCContentInside").append("<p><a class='RunningCourseScroll' href='#RunningCourseHeader' name='WEB'>" +
                                            data.d[i].Title + "</a></p>");
                                    if (categoryID == "2")//BASE
                                        $("#divSCContentInside").append("<p><a class='RunningCourseScroll' href='#RunningCourseHeader' name='BASE'>" +
                                            data.d[i].Title + "</a></p>");
                                    if (categoryID == "6")//Methodology
                                        $("#divSCContentInside").append("<p><a class='RunningCourseScroll' href='#RunningCourseHeader' name='OTHR'>" +
                                            data.d[i].Title + "</a></p>");
                                    if (categoryID == "8")//Kids
                                        $("#divSCContentInside").append("<p><a class='RunningCourseScroll' href='#RunningCourseHeader' name='KID'>" +
                                            data.d[i].Title + "</a></p>");
                                    if (categoryID == "12")//Single
                                        $("#divSCContentInside").append("<p><a href='SemiPrivate.aspx'>" +
                                            data.d[i].Title + "</a></p>");

                                    //else
                                    //    $("#divSCContentInside").append("<p><a href='../SpecialCourses/SpecialCourses.aspx?SpCourseID=" + data.d[i].ID + "'>" +
                                    //        data.d[i].Title + "</a></p>");
                                }

                                //.................... Set Padding And Other Necesseries

                                var paddingTop = (380 - (data.d.length * 50)) / 2;
                                $("#divSCContentInside").css("padding-top", paddingTop + "px");
                                $("#divSCContentInside").css("height", (380 - paddingTop) + "px");
                                ConvertToPersianDigits($("#divSCContentInside"));

                                $("#divSpecialCourseContent img[id='img" + categoryName + "']").fadeIn();
                                $("#divSCContentInside").fadeIn();
                            }
                            $("#divLoader").hide();
                        }
                    });
                }
            });

            //----------------------------------------------- Select A Course To Open Accourdion

            $('.Course').click(function (e) {

                //.................... Close Other Courses

                $(this).parent().find('.CourseDetails').each(function () {
                    $(this).slideUp(200);
                });

                $(this).parent().find('.Course').each(function () {
                    $(this).removeClass("Selected");
                });

                //.................... Show This Course Details

                $(this).next(".CourseDetails").slideToggle(300);
                $(this).addClass("Selected");
            });

            //----------------------------------------------- View PDF

            //انتخاب لینک با نام مودال
            $('a[name=pdfModal]').click(function (e) {
                //لغو حالت پیش فرض عملکرد لینک
                e.preventDefault();

                //دریافت آیدی عنصر پاپ آپ
                var id = $(this).attr('href');
                var fileName = id.toString().substr(0, id.lastIndexOf('#'));

                if (fileName != '') {
                    $.ajax({
                        type: "POST",
                        url: "/AjaxPages/PDF.aspx/ShowPDF",
                        data: "{fileName: '" + fileName + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (r) {
                            $("#divPopupViewPDFDialog").html(r.d);
                        }
                    });
                }

                id = id.toString().substr(id.lastIndexOf('#'));
                showModal(id);
            });



            //نمایش فایل های اکسل
            $('a[name=excel]').click(function (e) {
                //لغو حالت پیش فرض عملکرد لینک
                e.preventDefault();

                var id = "#divExcelFiles";
                $(id).slideDown(400);
            });




            //----------------------------------------------- View PDF

            $('a[class=modal]').click(function (e) {
                //لغو حالت پیش فرض عملکرد لینک
                e.preventDefault();

                //دریافت آیدی عنصر پاپ آپ
                var id = $(this).attr('href');
                var fileName = id.toString().substr(0, id.lastIndexOf('#'));

                if (fileName != '') {
                    $.ajax({
                        type: "POST",
                        url: "/AjaxPages/PDF.aspx/ShowPDF",
                        data: "{fileName: '" + fileName + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (r) {
                            $("#divPopupViewPDFDialog").html(r.d);
                        }
                    });
                }

                id = id.toString().substr(id.lastIndexOf('#'));
                showModal(id);
            });
        });
    </script>
