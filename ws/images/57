        <script type="text/javascript">
            $(document).ready(function () {
                //$("div#pv").scrollTop(0);


                // Convert to persian character
                ConvertToPersianDigits($(document));


                //----------------------------------------------- Sticky Menu

                $(window).scroll(function () {
                    var IsStuckyActivationHeight = 181;

                    if ($(this).scrollTop() > IsStuckyActivationHeight && !$('#divLoginArea').hasClass("IsStuck")) {
                        $('#divLoginArea').addClass("IsStuck");
                    } else if ($(this).scrollTop() <= IsStuckyActivationHeight && $('#divLoginArea').hasClass("IsStuck")) {
                        $('#divLoginArea').removeClass("IsStuck");
                    }
                })


               
            });

            //------------------------------------------------- Road Map
        </script>


        <script>

            $(".RunningCourse").click(function () {
                var tabID = $(this).attr("data-course");
                $("input[id$='Content_Hidden_Active_SPCourse_Type']").val(tabID);
                $("div[id^='div-SPCourses-']").fadeOut(200);
                $("#div-SPCourses-" + tabID).fadeIn(300);

                $("#ulSPCourses a").removeClass("Selected");
                $("a[href='#" + tabID + "']").addClass("Selected");
            });

            $("a[data-type=scroll]").click(function () {
                $(".TopMenuUL li  ul").hide();// css("opacity", "0");
                //setTimeout(function () {
                //    $(".TopMenuUL li  ul").css("opacity", "");
                //}, 1000);
            });
            $(".TopMenuUL li").mouseover(function () {

                $(".TopMenuUL li  ul").css("display", "");
            });
        </script>
        
    <script>
        $(document).ready(function () {
            //----------------------------------------------- Change CourseType

            $('a[name=courseType]').click(function (e) {
                e.preventDefault();
                itemIndex = $(this).attr('href');
                itemLst = $(this).attr('data-lst');
                itemIndex = itemIndex.substr(1);

                var itemName = "";
                if (itemIndex == "0")
                    itemName = "#ctl00_ctl00_Content_Content_" + itemLst + "_ctrl0_";
                else
                    itemName = "#ctl00_ctl00_Content_Content_" + itemLst + "_ctrl" + (2 * parseInt(itemIndex)).toString() + "_";
                switch ($(this).css("color")) {
                    case 'rgb(0, 128, 0)':
                        $(itemName + "LblCourseType").html("خصوصی - آنلاین");
                        $(itemName + "LblCourseType").css("background-color", $(this).css("color"));
                        $(itemName + "LblCourseTime").html("با مشورت استاد");
                        $(itemName + "LblCourseTime").css("color", $(this).css("color"));
                        $(itemName + "LblPrice").html($(itemName + "HiddenPrivatePrice").val() + " تومان");
                        $(itemName + "LblPrice").css("color", $(this).css("color"));
                        $(itemName + "LblPriceTitle").html("هزینه هر ساعت");
                        break;

                    case 'rgb(255, 165, 0)':
                        $(itemName + "LblCourseType").html("برون سازمانی");
                        $(itemName + "LblCourseType").css("background-color", $(this).css("color"));
                        $(itemName + "LblCourseTime").html($(itemName + "HiddenCourseTime").val() + " ساعت");
                        $(itemName + "LblCourseTime").css("color", $(this).css("color"));
                        $(itemName + "LblPrice").html($(itemName + "HiddenPublicPrice").val() + " تومان");
                        $(itemName + "LblPrice").css("color", $(this).css("color"));
                        $(itemName + "LblPriceTitle").html("شهریه هر نفر");
                        break;
                }
                ConvertToPersianDigits($(itemName + "LblCourseTime"));
                ConvertToPersianDigits($(itemName + "LblPrice"));
            });

            $('a[data-name=courseTypeLink2]').click(function (e) {
                $('a[name=courseType]').each(function () {

                    //e.preventDefault();
                    itemIndex = $(this).attr('href');
                    itemLst = $(this).attr('data-lst');
                    itemIndex = itemIndex.substr(1);
                    debugger;
                    var itemName = "";
                    if (itemIndex == "0")
                        itemName = "#ctl00_ctl00_Content_Content_" + itemLst + "_ctrl0_";
                    else
                        itemName = "#ctl00_ctl00_Content_Content_" + itemLst + "_ctrl" + (2 * parseInt(itemIndex)).toString() + "_";

                    $(itemName + "LblCourseType").html("برون سازمانی");
                    $(itemName + "LblCourseType").css("background-color", "#ffa500");
                    $(itemName + "LblCourseTime").html($(itemName + "HiddenCourseTime").val() + " ساعت");
                    $(itemName + "LblCourseTime").css("color", "#ffa500");
                    $(itemName + "LblPrice").html($(itemName + "HiddenPublicPrice").val() + " تومان");
                    $(itemName + "LblPrice").css("color", "#ffa500");
                    $(itemName + "LblPriceTitle").html("شهریه هر نفر");



                    ConvertToPersianDigits($(itemName + "LblCourseTime"));
                    ConvertToPersianDigits($(itemName + "LblPrice"));
                });
            });

            $('a[data-name=courseTypeLink1]').click(function (e) {
                $('a[name=courseType]').each(function () {

                    //e.preventDefault();
                    itemIndex = $(this).attr('href');
                    itemLst = $(this).attr('data-lst');
                    itemIndex = itemIndex.substr(1);

                    var itemName = "";
                    if (itemIndex == "0")
                        itemName = "#ctl00_ctl00_Content_Content_" + itemLst + "_ctrl0_";
                    else
                        itemName = "#ctl00_ctl00_Content_Content_" + itemLst + "_ctrl" + (2 * parseInt(itemIndex)).toString() + "_";

                    $(itemName + "LblCourseType").html("خصوصی - آنلاین");
                    $(itemName + "LblCourseType").css("background-color", "#008000");
                    $(itemName + "LblCourseTime").html("با مشورت استاد");
                    $(itemName + "LblCourseTime").css("color", "#008000");
                    $(itemName + "LblPrice").html($(itemName + "HiddenPrivatePrice").val() + " تومان");
                    $(itemName + "LblPrice").css("color", "#008000");
                    $(itemName + "LblPriceTitle").html("هزینه هر ساعت");




                    ConvertToPersianDigits($(itemName + "LblCourseTime"));
                    ConvertToPersianDigits($(itemName + "LblPrice"));
                });
            });
        });
    </script>
    
